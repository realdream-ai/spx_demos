
onKey KeyUp, => {
	Camera.changeXYpos 0, 10
}

onKey KeyDown, => {
	Camera.changeXYpos 0, -10
}

onKey KeyLeft, => {
	Camera.changeXYpos -10, 0
}

onKey KeyRight, => {
	Camera.changeXYpos 10, 0
}

onKey KeyN, => {
    zoom := Camera.zoom
    zoom+= 0.1
	Camera.setZoom zoom
}

onKey KeyM, => {
    zoom := Camera.zoom
    zoom+= -0.1
	Camera.setZoom zoom
}
